<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="JoinDao">
	
	<!-- result = session.insert("JoinProcess", memberinfo); -->
	<insert id="JoinProcess" parameterType="memberinfo">
		insert into memberinfo values (#{id}, #{password}, #{nickname}, 30000000, sysdate, sysdate)
	</insert>
	
	<!-- result = session.selectOne("IDchk", memberinfo); -->
	<select id="IDchk" parameterType="memberinfo" resultType="int">
		select count(*)
		from memberinfo
		where id = #{id}
	</select>
	



</mapper>


